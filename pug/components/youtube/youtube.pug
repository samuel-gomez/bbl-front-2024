mixin Youtube({list})
    article(class=`${prefix}-youtube`)
        each item in list
            iframe(width='560' height='315' src=`https://www.youtube.com/embed/${item}` title='YouTube video player' frameborder='0' allow='accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share' allowfullscreen='')

